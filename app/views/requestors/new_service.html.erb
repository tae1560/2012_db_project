<%= render "menu" %>

<%= form_for @service, :html => {:multipart => true} do |f| %>
    이름 : <%= f.text_field :name %>
    모집마감시간 : <%= f.text_field :due_date %>
    서비스 기획서 : <%= file_field :service_file, :file %>
    <%= f.submit "등록" %>
<% end %>

<% if notice %>
    <%= notice %>
    <% notice = nil %>
<% end %>
